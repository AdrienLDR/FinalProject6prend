<?import javafx.scene.layout.*?>

<HBox xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo.HelloApplication">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>

    <VBox alignment="CENTER" spacing="10.0">
        <Label fx:id="welcomeText" />
        <AnchorPane prefHeight="200.0" prefWidth="200.0">
            <children>
                <Button fx:id="onePlayerButton" layoutX="81.0" layoutY="134.0" styleClass="square-button" text="1 player" />
                <Button fx:id="twoPlayersButton" layoutX="79.0" layoutY="100.0" styleClass="square-button" text="2 players" />
                <Label layoutX="86.0" layoutY="41.0" text="7 qui prend" />
            </children>
        </AnchorPane>
    </VBox>
</HBox>
